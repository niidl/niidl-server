// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Projects {
  project_id   Int    @id @default(autoincrement())
  project_name String
  description  String
  repo_url     String
  owner        String
}
